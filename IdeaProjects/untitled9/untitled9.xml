<?xml version="1.0" encoding="UTF-8"?>
<project name="untitled9" default="all">
  
  
  <property file="untitled9.properties"/>
  <!-- Uncomment the following property if no tests compilation is needed -->
  <!-- 
  <property name="skip.tests" value="true"/>
   -->
  
  <!-- Compiler options -->
  
  <property name="compiler.debug" value="on"/>
  <property name="compiler.generate.no.warnings" value="off"/>
  <property name="compiler.args" value=""/>
  <property name="compiler.max.memory" value="700m"/>
  <patternset id="ignored.files">
    <exclude name="**/*.hprof/**"/>
    <exclude name="**/*.pyc/**"/>
    <exclude name="**/*.pyo/**"/>
    <exclude name="**/*.rbc/**"/>
    <exclude name="**/*~/**"/>
    <exclude name="**/.DS_Store/**"/>
    <exclude name="**/.bundle/**"/>
    <exclude name="**/.git/**"/>
    <exclude name="**/.hg/**"/>
    <exclude name="**/.svn/**"/>
    <exclude name="**/.tox/**"/>
    <exclude name="**/CVS/**"/>
    <exclude name="**/RCS/**"/>
    <exclude name="**/SCCS/**"/>
    <exclude name="**/__pycache__/**"/>
    <exclude name="**/_svn/**"/>
    <exclude name="**/rcs/**"/>
    <exclude name="**/vssver.scc/**"/>
    <exclude name="**/vssver2.scc/**"/>
  </patternset>
  <patternset id="library.patterns">
    <include name="*.war"/>
    <include name="*.ear"/>
    <include name="*.apk"/>
    <include name="*.zip"/>
    <include name="*.swc"/>
    <include name="*.ane"/>
    <include name="*.egg"/>
    <include name="*.jar"/>
  </patternset>
  <patternset id="compiler.resources">
    <exclude name="**/?*.java"/>
    <exclude name="**/?*.form"/>
    <exclude name="**/?*.class"/>
    <exclude name="**/?*.groovy"/>
    <exclude name="**/?*.scala"/>
    <exclude name="**/?*.flex"/>
    <exclude name="**/?*.kt"/>
    <exclude name="**/?*.clj"/>
    <exclude name="**/?*.aj"/>
  </patternset>
  
  <!-- JDK definitions -->
  
  <property name="jdk.bin.1.7" value="${jdk.home.1.7}/bin"/>
  <path id="jdk.classpath.1.7">
    <fileset dir="${jdk.home.1.7}">
      <include name="jre/lib/charsets.jar"/>
      <include name="jre/lib/deploy.jar"/>
      <include name="jre/lib/javaws.jar"/>
      <include name="jre/lib/jce.jar"/>
      <include name="jre/lib/jfr.jar"/>
      <include name="jre/lib/jfxrt.jar"/>
      <include name="jre/lib/jsse.jar"/>
      <include name="jre/lib/management-agent.jar"/>
      <include name="jre/lib/plugin.jar"/>
      <include name="jre/lib/resources.jar"/>
      <include name="jre/lib/rt.jar"/>
      <include name="jre/lib/ext/access-bridge-64.jar"/>
      <include name="jre/lib/ext/dnsns.jar"/>
      <include name="jre/lib/ext/jaccess.jar"/>
      <include name="jre/lib/ext/localedata.jar"/>
      <include name="jre/lib/ext/sunec.jar"/>
      <include name="jre/lib/ext/sunjce_provider.jar"/>
      <include name="jre/lib/ext/sunmscapi.jar"/>
      <include name="jre/lib/ext/zipfs.jar"/>
    </fileset>
  </path>
  
  <property name="project.jdk.home" value="${jdk.home.1.7}"/>
  <property name="project.jdk.bin" value="${jdk.bin.1.7}"/>
  <property name="project.jdk.classpath" value="jdk.classpath.1.7"/>
  
  
  <!-- Project Libraries -->
  
  <path id="library.maven:_aopalliance:aopalliance:1.0.classpath">
    <pathelement location="${path.variable.maven_repository}/aopalliance/aopalliance/1.0/aopalliance-1.0.jar"/>
  </path>
  
  <path id="library.maven:_commons-logging:commons-logging:1.1.3.classpath">
    <pathelement location="${path.variable.maven_repository}/commons-logging/commons-logging/1.1.3/commons-logging-1.1.3.jar"/>
  </path>
  
  <path id="library.maven:_javax.servlet.jsp:jsp-api:2.1.classpath">
    <pathelement location="${path.variable.maven_repository}/javax/servlet/jsp/jsp-api/2.1/jsp-api-2.1.jar"/>
  </path>
  
  <path id="library.maven:_javax.servlet:servlet-api:2.5.classpath">
    <pathelement location="${path.variable.maven_repository}/javax/servlet/servlet-api/2.5/servlet-api-2.5.jar"/>
  </path>
  
  <path id="library.maven:_junit:junit:4.11.classpath">
    <pathelement location="${path.variable.maven_repository}/junit/junit/4.11/junit-4.11.jar"/>
  </path>
  
  <path id="library.maven:_org.hamcrest:hamcrest-core:1.3.classpath">
    <pathelement location="${path.variable.maven_repository}/org/hamcrest/hamcrest-core/1.3/hamcrest-core-1.3.jar"/>
  </path>
  
  <path id="library.maven:_org.springframework:spring-aop:4.1.1.release.classpath">
    <pathelement location="${path.variable.maven_repository}/org/springframework/spring-aop/4.1.1.RELEASE/spring-aop-4.1.1.RELEASE.jar"/>
  </path>
  
  <path id="library.maven:_org.springframework:spring-beans:4.1.1.release.classpath">
    <pathelement location="${path.variable.maven_repository}/org/springframework/spring-beans/4.1.1.RELEASE/spring-beans-4.1.1.RELEASE.jar"/>
  </path>
  
  <path id="library.maven:_org.springframework:spring-context:4.1.1.release.classpath">
    <pathelement location="${path.variable.maven_repository}/org/springframework/spring-context/4.1.1.RELEASE/spring-context-4.1.1.RELEASE.jar"/>
  </path>
  
  <path id="library.maven:_org.springframework:spring-core:4.1.1.release.classpath">
    <pathelement location="${path.variable.maven_repository}/org/springframework/spring-core/4.1.1.RELEASE/spring-core-4.1.1.RELEASE.jar"/>
  </path>
  
  <path id="library.maven:_org.springframework:spring-expression:4.1.1.release.classpath">
    <pathelement location="${path.variable.maven_repository}/org/springframework/spring-expression/4.1.1.RELEASE/spring-expression-4.1.1.RELEASE.jar"/>
  </path>
  
  <path id="library.maven:_org.springframework:spring-test:4.1.1.release.classpath">
    <pathelement location="${path.variable.maven_repository}/org/springframework/spring-test/4.1.1.RELEASE/spring-test-4.1.1.RELEASE.jar"/>
  </path>
  
  <path id="library.maven:_org.springframework:spring-web:4.1.1.release.classpath">
    <pathelement location="${path.variable.maven_repository}/org/springframework/spring-web/4.1.1.RELEASE/spring-web-4.1.1.RELEASE.jar"/>
  </path>
  
  <path id="library.maven:_org.springframework:spring-webmvc:4.1.1.release.classpath">
    <pathelement location="${path.variable.maven_repository}/org/springframework/spring-webmvc/4.1.1.RELEASE/spring-webmvc-4.1.1.RELEASE.jar"/>
  </path>
  <!-- Register Custom Compiler Taskdefs -->
  <property name="javac2.home" value="${idea.home}/lib"/>
  <path id="javac2.classpath">
    <pathelement location="${javac2.home}/javac2.jar"/>
    <pathelement location="${javac2.home}/jdom.jar"/>
    <pathelement location="${javac2.home}/asm-all.jar"/>
    <pathelement location="${javac2.home}/jgoodies-forms.jar"/>
  </path>
  <target name="register.custom.compilers">
    <taskdef name="javac2" classname="com.intellij.ant.Javac2" classpathref="javac2.classpath"/>
    <taskdef name="instrumentIdeaExtensions" classname="com.intellij.ant.InstrumentIdeaExtensions" classpathref="javac2.classpath"/>
  </target>
  
  <!-- Modules -->
  
  <import file="${basedir}/module_untitled9.xml"/>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.untitled9, clean.artifact.untitled9:war_exploded" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.untitled9" description="build all modules"/>
  
  <target name="init.artifacts">
    <property name="artifacts.temp.dir" value="${basedir}/__artifacts_temp"/>
    <property name="artifact.output.untitled9:war" value="${basedir}/target"/>
    <property name="artifact.output.untitled9:war_exploded" value="${basedir}/target/untitled9"/>
    <mkdir dir="${artifacts.temp.dir}"/>
    <property name="temp.jar.path.untitled9.war" value="${artifacts.temp.dir}/untitled9.war"/>
  </target>
  
  <target name="clean.artifact.untitled9:war_exploded" description="clean untitled9:war exploded artifact output">
    <delete dir="${artifact.output.untitled9:war_exploded}"/>
  </target>
  
  <target name="artifact.untitled9:war" depends="init.artifacts, artifact.untitled9:war_exploded" description="Build &#39;untitled9:war&#39; artifact">
    <property name="artifact.temp.output.untitled9:war" value="${artifacts.temp.dir}/untitled9_war"/>
    <mkdir dir="${artifact.temp.output.untitled9:war}"/>
    <zip destfile="${temp.jar.path.untitled9.war}">
      <zipfileset dir="${artifact.output.untitled9:war_exploded}"/>
    </zip>
    <copy file="${temp.jar.path.untitled9.war}" tofile="${artifact.temp.output.untitled9:war}/untitled9.war"/>
  </target>
  
  <target name="artifact.untitled9:war_exploded" depends="init.artifacts, compile.module.untitled9" description="Build &#39;untitled9:war exploded&#39; artifact">
    <mkdir dir="${artifact.output.untitled9:war_exploded}"/>
    <mkdir dir="${artifact.output.untitled9:war_exploded}/WEB-INF"/>
    <mkdir dir="${artifact.output.untitled9:war_exploded}/WEB-INF/classes"/>
    <copy todir="${artifact.output.untitled9:war_exploded}/WEB-INF/classes">
      <fileset dir="${untitled9.output.dir}"/>
    </copy>
    <mkdir dir="${artifact.output.untitled9:war_exploded}/WEB-INF/lib"/>
    <copy file="${path.variable.maven_repository}/org/springframework/spring-core/4.1.1.RELEASE/spring-core-4.1.1.RELEASE.jar" tofile="${artifact.output.untitled9:war_exploded}/WEB-INF/lib/spring-core-4.1.1.RELEASE.jar"/>
    <copy file="${path.variable.maven_repository}/commons-logging/commons-logging/1.1.3/commons-logging-1.1.3.jar" tofile="${artifact.output.untitled9:war_exploded}/WEB-INF/lib/commons-logging-1.1.3.jar"/>
    <copy file="${path.variable.maven_repository}/org/springframework/spring-web/4.1.1.RELEASE/spring-web-4.1.1.RELEASE.jar" tofile="${artifact.output.untitled9:war_exploded}/WEB-INF/lib/spring-web-4.1.1.RELEASE.jar"/>
    <copy file="${path.variable.maven_repository}/org/springframework/spring-aop/4.1.1.RELEASE/spring-aop-4.1.1.RELEASE.jar" tofile="${artifact.output.untitled9:war_exploded}/WEB-INF/lib/spring-aop-4.1.1.RELEASE.jar"/>
    <copy file="${path.variable.maven_repository}/aopalliance/aopalliance/1.0/aopalliance-1.0.jar" tofile="${artifact.output.untitled9:war_exploded}/WEB-INF/lib/aopalliance-1.0.jar"/>
    <copy file="${path.variable.maven_repository}/org/springframework/spring-beans/4.1.1.RELEASE/spring-beans-4.1.1.RELEASE.jar" tofile="${artifact.output.untitled9:war_exploded}/WEB-INF/lib/spring-beans-4.1.1.RELEASE.jar"/>
    <copy file="${path.variable.maven_repository}/org/springframework/spring-context/4.1.1.RELEASE/spring-context-4.1.1.RELEASE.jar" tofile="${artifact.output.untitled9:war_exploded}/WEB-INF/lib/spring-context-4.1.1.RELEASE.jar"/>
    <copy file="${path.variable.maven_repository}/javax/servlet/servlet-api/2.5/servlet-api-2.5.jar" tofile="${artifact.output.untitled9:war_exploded}/WEB-INF/lib/servlet-api-2.5.jar"/>
    <copy file="${path.variable.maven_repository}/org/springframework/spring-webmvc/4.1.1.RELEASE/spring-webmvc-4.1.1.RELEASE.jar" tofile="${artifact.output.untitled9:war_exploded}/WEB-INF/lib/spring-webmvc-4.1.1.RELEASE.jar"/>
    <copy file="${path.variable.maven_repository}/org/springframework/spring-expression/4.1.1.RELEASE/spring-expression-4.1.1.RELEASE.jar" tofile="${artifact.output.untitled9:war_exploded}/WEB-INF/lib/spring-expression-4.1.1.RELEASE.jar"/>
    <mkdir dir="${artifact.output.untitled9:war_exploded}/META-INF"/>
    <copy file="${basedir}/target/untitled9/META-INF/MANIFEST.MF" tofile="${artifact.output.untitled9:war_exploded}/META-INF/MANIFEST.MF"/>
    <copy todir="${artifact.output.untitled9:war_exploded}">
      <fileset dir="${basedir}/src/main/webapp"/>
    </copy>
    <mkdir dir="${artifact.output.untitled9:war_exploded}/WEB-INF"/>
    <copy file="${basedir}/src/main/webapp/WEB-INF/web.xml" tofile="${artifact.output.untitled9:war_exploded}/WEB-INF/web.xml"/>
  </target>
  
  <target name="build.all.artifacts" depends="artifact.untitled9:war, artifact.untitled9:war_exploded" description="Build all artifacts">
    <mkdir dir="${artifact.output.untitled9:war}"/>
    <copy todir="${artifact.output.untitled9:war}">
      <fileset dir="${artifact.temp.output.untitled9:war}"/>
    </copy>
    
    <!-- Delete temporary files -->
    <delete dir="${artifacts.temp.dir}"/>
  </target>
  
  <target name="all" depends="build.modules, build.all.artifacts" description="build all"/>
</project>